library(MASS)
library(tidyverse)
data("mtcars")
model <- lm(mpg ~ wt + hp, data = mtcars)
resid(model)
qqplot()
qqnorm(resid(model))
summary(model)
shapiro.test(resid(model))
cooks.distance(model)
dane_filtr <- mtcars|>
  filter(cooks.distance(model)<=4/32)
model2 <- lm(mpg ~ wt + hp, data = dane_filtr)
qqnorm(resid(model2))
shapiro.test(resid(model2))
